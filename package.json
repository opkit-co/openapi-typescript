{
  "name": "@opkit/openapi-typescript",
  "description": "Generate TypeScript types from Swagger OpenAPI specs",
  "version": "1.0.0",
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 7.0.0"
  },
  "license": "MIT",
  "bin": {
    "openapi-typescript": "bin/cli.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "module",
  "files": [
    "bin",
    "dist",
    "src"
  ],
  "keywords": [
    "swagger",
    "typescript",
    "ts",
    "dts",
    "openapi",
    "codegen",
    "generation",
    "openapi 3",
    "node"
  ],
  "publishConfig": {
    "access": "restricted",
    "scope": "@opkit"
  },
  "scripts": {
    "build": "rm -rf dist && ./node_modules/.bin/tsc",
    "format": "node_modules/.bin/prettier -w .",
    "lint": "node_modules/.bin/eslint .",
    "prepare": "yarn build",
    "pregenerate": "yarn build",
    "test": "node_modules/.bin/mocha --recursive --timeout 30000",
    "test:coverage": "nyc --reporter=lcov node_modules/.bin/mocha --recursive --timeout 30000",
    "version": "yarn build",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "mime": "^3.0.0",
    "prettier": "^2.6.2",
    "tiny-glob": "^0.2.9",
    "undici": "^5.4.0",
    "yargs-parser": "^21.0.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/mime": "^2.0.3",
    "@types/node": "^17.0.38",
    "@types/prettier": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "chai": "^4.3.6",
    "del-cli": "^4.0.1",
    "eol": "^0.9.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "typescript": "^4.7.2"
  }
}
